{
  "name": "microsoft-excel",
  "version": "16.0.0",
  "description": "Microsoft Excel - Powerful spreadsheet application for data analysis and visualization",
  "main": "src/web/frontend/src/components/App.tsx",
  "scripts": {
    "start": "webpack-dev-server --config config/webpack.config.js",
    "build": "webpack --config config/webpack.config.js",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "build:desktop": "electron-builder",
    "build:web": "webpack --config config/webpack.config.js --env.platform=web",
    "build:mobile": "react-native build-android && react-native build-ios"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/excel.git"
  },
  "keywords": [
    "excel",
    "spreadsheet",
    "data-analysis",
    "charts",
    "pivot-tables",
    "microsoft"
  ],
  "author": "Microsoft Corporation",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/microsoft/excel/issues"
  },
  "homepage": "https://www.microsoft.com/excel",
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "redux": "^4.1.0",
    "react-redux": "^7.2.4",
    "typescript": "^4.3.5",
    "chart.js": "^3.5.0",
    "signalr": "^2.4.1",
    "office-ui-fabric-react": "^7.174.1"
  },
  "devDependencies": {
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.24.0",
    "jest": "^27.0.6",
    "ts-jest": "^27.0.3",
    "webpack": "^5.44.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "typescript": "^4.3.5",
    "prettier": "^2.3.2",
    "electron": "^13.1.7",
    "electron-builder": "^22.11.7",
    "react-native": "^0.64.2",
    "react-native-cli": "^2.0.1"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}

